<%= link_to 'Create Post', new_blog_path %>
<div>
<%= link_to 'View', blog_path(blog.id) %>
<%= link_to 'Edit', edit_blog_path(blog.id), data: {confirm: "Confirm Edit!"} %>
<%= link_to 'Delete', blog_path(blog.id), method: :delete, data: {confirm: "Confirm Delete!"} %>
  <div>
    <% @blogs.each do |b| %>
      <%= image_tag b.user.image.url %>
      <%= b.user.name %><br>
      <%= image_tag b.image.url %><br>
      <%= b.content %>
      <% end %>
  </div>
</div>
